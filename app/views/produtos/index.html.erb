<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Nome</th>
          <th>Descrição</th>
          <th>Quantidade</th>
          <th>Preço</th>
        </tr>
      </thead>
      <tbody>
        <% @td_produtos.each do |p| %>
        <tr>
          <td><%= p.nome %></td>
          <td><%= p.descricao %></td>
          <td><%= p.quantidade %></td>
          <td><%= p.preco %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </body>
</html>
